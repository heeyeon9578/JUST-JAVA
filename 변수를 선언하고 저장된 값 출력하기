public class Ch3Ex1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num1 = 12;
		int num2 = 20;
		int result = num1+num2;
		
		System.out.println(num1+"+"+ num2);
		System.out.printf("%d+%d = %d", num1, num2, result);
		

	}

}
