public class Ch3Ex3 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		boolean result = true;
		float num1 = 32.87f;
		double num2 = 3.14E5;
		
		System.out.printf("result: %s \n", result);
		System.out.printf("num1: %f \n", num1);
		System.out.printf("num2: %f \n", num2);
		

	}

}
